    <avalonia:DoubleToLog10Converter x:Key="Log10Converter" />
    <avalonia:EqualityToBooleanConverter x:Key="EqualityConverter" />

    <avalonia:RoundedValueConverter x:Key="RoundedValueConverter" Precision="2" />
    <avalonia:VisibilityToBoolConverter x:Key="TrueIfVisibleConverter"
                                        Inverted="False" Not="False" />
    <avalonia:VisibilityToBoolConverter x:Key="TrueIfNotVisibleConverter"
                                        Inverted="False" Not="True" />
    <avalonia:VisibilityToBoolConverter x:Key="VisibleIfTrueConverter"
                                        Inverted="True" Not="False" />
    <avalonia:VisibilityToBoolConverter x:Key="VisibleIfNotTrueConverter"
                                        Inverted="True" Not="True" />

