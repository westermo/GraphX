<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:models="clr-namespace:ShowcaseApp.Avalonia.Models"
                    xmlns:avalonia="clr-namespace:Westermo.GraphX.Controls.Avalonia;assembly=Westermo.GraphX.Controls.Avalonia">
    <models:ValueToEditorImageConverter x:Key="ImageConverter" />

    <avalonia:VisibilityToBoolConverter
        x:Key="BooleanToVisibility"
        Inverted="True"
        Not="True" />

    <avalonia:GeometryToPathGeometryConverter x:Key="GeometryToPathGeometry" />
    <models:BoolToColorConverter TrueColor="LightBlue" FalseColor="Yellow" x:Key="ColorConverter" />
</ResourceDictionary>